<?xml version="1.0"?>
<!--
    Scale is a double value
    range is (0.5, 10)
-->
<words>
    <word scale="4.4">fabulous</word>
    <word scale="5">perfect</word>
    <word scale="3.5">wondrous</word>
    <word scale="-1">awkward</word>
    <word scale="-1.2">bad</word>
    <word scale="-1.3">crap</word>
    <word scale="-3.7">masochistic</word>
    <word scale="-0.8">plain</word>
</words>